    "@ember/render-modifiers": "^1.0.2",
    "@ember/test-helpers": "^2.2.8",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.4",
    "@kosmos/remotestorage-module-kosmos": "^2.2.0",
