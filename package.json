    "npm-run-all": "^4.1.5",
    "postcss": "8.4.6",
    "postcss-scss": "4.0.3",
    "qunit": "^2.18.0",
    "qunit-dom": "^1.6.0",
    "remotestoragejs": "^2.0.0-beta.3",
    "tailwindcss": "^3.0.23"
  },
